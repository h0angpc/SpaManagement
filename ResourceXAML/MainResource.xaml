<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:SpaManagement.ViewModel"
                    xmlns:views="clr-namespace:SpaManagement.Views">
    <DataTemplate DataType="{x:Type viewmodel:HomeViewModel}">
        <views:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:CustomerViewModel}">
        <views:CustomerView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:PaymentViewModel}">
        <views:PaymentView/>
    </DataTemplate>
    <viewmodel:MainViewModel x:Key="MainVM"/>
    <viewmodel:LoginViewModel x:Key="LoginVM"/>
    <viewmodel:AddCustomerViewModel x:Key="AddCusomterVM"/>
</ResourceDictionary>